}

QAbstractItemView::indicator:hover
{
	border-color: #007f7f;
}

QAbstractItemView::indicator
{
	background: qlineargradient(
				x1: 0, y1: 0, x2: 0, y2: 1,
				stop: 0 #EEEEEE, stop: 0.6 #E0E0E0,
                stop: 0.7 #E8E8E8, stop: 1.0 #E3E3E3);

    border: 1px solid #D3D3D3;
}

QAbstractItemView::indicator:checked
{
	background: qlineargradient(
				x1: 0, y1: 0, x2: 0, y2: 1,
				stop: 0 #7f9cc1, 
                stop: 1.0 #eff2f8);
                
    border-color: #007f7f;
}





QTabWidget QWidget
{
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
								   stop: 0 #F0FFF0, stop: 1 #FFFFFF);
}

QTabWidget::pane
{
	border: 1px solid #B0B0B0;
}

QTabBar::tab
{
	background: qlineargradient(
				x1: 0, y1: 0, x2: 0, y2: 1,
				stop: 0 #FFFFFF,
                stop: 1.0 #F0FFF0);

	border: 1px solid #B0B0B0;
	border-bottom-style: none;
	
	padding: 4px 8px 4px 8px;
}

QTabBar::tab:last
{
	border-top-right-radius: 3px;
}

QTabBar::tab:last:selected
{
	border-right-color: #B0B0B0;
}

QTabBar::tab:first:selected
{
	border-left-color: #B0B0B0;
}

QTabBar::tab:hover
{
	background: qlineargradient(
				x1: 0, y1: 0, x2: 0, y2: 1,
				stop: 0 #EEFFFF, stop: 0.5 #E0F0F0,
                stop: 0.51 #8fAcD1, stop: 1.0 #eff2f8);
                
	border-color: #007f7f;
}

QTabBar::tab:selected
{       
	background: qlineargradient(
				x1: 0, y1: 0, x2: 0, y2: 1,
				stop: 0 #E0F0F0,
                stop: 1.0 #FFFFFF);
                
	border-color: #007f7f;
}





QToolTip
{
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
								   stop: 0 #F0FFF0, stop: 1 #FFFFFF);						   
								   
	border-radius: 3px;
	border: 1px solid #4b8598;
}





QHeaderView
{
    background: qlineargradient(
                x1: 0, y1: 0, x2: 0, y2: 1,
                stop: 0 #E0F0F0,
                stop: 1.0 #FFFFFF);
}

QHeaderView::section
{
    background: qlineargradient(
                x1: 0, y1: 0, x2: 0, y2: 1,
                stop: 0 #E0F0F0,
                stop: 1.0 #FFFFFF);

    border: 1px solid #D3E3E3;

    padding: 4px 4px 4px 4px;
}

QHeaderView::section:hover
{
    background: qlineargradient(
                x1: 0, y1: 0, x2: 0, y2: 1,
                stop: 0 #EEFFFF, stop: 0.5 #E0F0F0,
                stop: 0.51 #8fAcD1, stop: 1.0 #eff2f8);
                
    border-color: #007f7f;
}

QHeaderView::down-arrow {
    width: 16px;
    height: 16px;
    image: url(:/images/combo_down_arrow.png);
}

QHeaderView::up-arrow {
    width: 16px;
    height: 16px;
    image: url(:/images/combo_up_arrow.png);
} 



QLabel[kind="header1"]
{
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
								   stop: 0 #6799B3, stop: 0.3 #377a90, 
								   stop: 0.31 #0e5e73, stop: 1 #4b8598);
	color: white;								   
								   
	min-height: 36px;
	border-radius: 3px;
	border: 1px solid #4b8598;
	
	font-weight: bold;
	font-size: 12px;
}

QLabel[kind="header2"]
{
	font-weight: bold;
	font-size: 12px;
	
	border: 1px solid #4b8598;
	border-radius: 3px;
	border-bottom-right-radius: 0px;
	border-bottom-left-radius: 0px;
	border-bottom: none;
	
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
								   stop: 0 #F0FFF0, stop: 1 #FFFFFF);	
	color: #4b8598;									   
}

QLabel[kind="helplabel"]
{
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
								   stop: 0 #F0FFF0, stop: 1 #FFFFFF);						   
								   
	border-radius: 3px;
	border: 1px solid #4b8598;
}



QToolButton[kind="toolbar"]
{
	background-color: transparent; 
	
	border-width: 0px; 
	border-left-width: 1px;
	border-right-width: 1px;
	border-style: solid;
	border-color: transparent;
	
	font-weight: bold;
	text-align: center;
	
	color: white;
}

QToolButton[kind="toolbar"]:hover, QToolButton[kind="toolbar"]:hover:checked
{
	background: qradialgradient(
				cx:0.5, cy:1, radius: 0.5, fx:0.5, fy:1.3,
				stop: 0 cyan, stop: 1.0 transparent);
                
	color: yellow;
	
	border-color: #0e536e;
}

QToolButton[kind="toolbar"]:!enabled
{
	color: gray;
}

QToolButton[kind="toolbar"]:checked
{
	padding-top: 5px;
	color: yellow;
	
	background: qradialgradient(
				cx:0.5, cy:1, radius: 0.5, fx:0.5, fy:1.3,
				stop: 0 #00bbbb, stop: 1.0 transparent);
								
}

QToolButton[kind="toolbar"]::menu-button
{
	background-color: transparent;
	padding-right: 8px;
}
QToolButton[kind="toolbar"]::menu-button:hover
{
	color: yellow;
}
QToolButton[kind="toolbar"]::menu-button:pressed
{
	padding-top: 2px;
}


QToolButton[kind="toolbar"][style="style1"]:hover
{
	color: lightgreen;
}

QToolButton[kind="toolbar"][style="style2"]:hover
{
	color: cyan;
}

QToolButton[kind="toolbar"][style="style2"]:checked
{
	color: cyan;
}



#iisStringListEditDialog QToolButton
